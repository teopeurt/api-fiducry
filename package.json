{
  "name": "fiducry-api",
  "version": "0.0.1",
  "description": "Fiducry API",
  "main": "src/index.ts",
  "scripts": {
    "start": "node src/index.ts",
    "devmon": "nodemon",
    "dev": "cross-env NODE_ENV=development DEBUG=1 ./node_modules/.bin/nodemon --watch server src/index.ts",
    "deploy": "now -e REDIS_URL=@redis_url && now alias",
    "precommit": "lint-staged",
    "build": "tsc -p . --pretty"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "engine": {
    "node": "8.11.4"
  },
  "now": {
    "alias": "api.fiducry.com"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/teopeurt/fiducry-api"
  },
  "author": "Don <don@teopeurt.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://gitlab.com/teopeurt/fiducry-api/issues"
  },
  "homepage": "https://gitlab.com/teopeurt/fiducry-api#readme",
  "dependencies": {
    "autobahn": "^0.11.1",
    "bluebird": "^3.4.7",
    "cross-env": "3.1.3",
    "dotenv": "2.0.0",
    "http-errors": "1.5.1",
    "isomorphic-fetch": "2.2.1",
    "koa": "2.0.0",
    "koa-bodyparser": "2.3.0",
    "koa-compress": "1.0.9",
    "koa-convert": "1.2.0",
    "koa-helmet": "2.0.0",
    "koa-logger": "1.3.0",
    "koa-mount": "2.0.0",
    "koa-ratelimit": "^4.0.0",
    "koa-router": "7.0.1",
    "lodash": "^4.17.4",
    "ms": "0.7.2",
    "now-logs": "^0.0.7",
    "pm2": "^2.4.0",
    "poloniex-api-node": "^1.9.0",
    "reconnecting-websocket": "^3.0.3",
    "redis": "^2.6.4",
    "safestart": "1.1.0",
    "semver": "^5.3.0",
    "socks-proxy-agent": "^4.0.1",
    "uws": "^10.148.1",
    "websocket": "^1.0.24"
  },
  "devDependencies": {
    "@types/events": "^3.0.0",
    "@types/koa": "^2.0.48",
    "@types/node": "^11.13.8",
    "husky": "^0.14.3",
    "lint-staged": "^4.0.0",
    "nodemon": "1.11.0",
    "prettier": "^1.5.2",
    "typescript": "^3.4.5"
  }
}
